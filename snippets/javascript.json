{
  "console.log": {
    "prefix": ["clog"],
    "body": ["console.log($1)"],
    "description": "console.log"
  },
  "describe": {
    "prefix": ["describe", "jtest"],
    "body": [
      "describe(\"$1\", function () {",
      "  it(\"$2\", function () {",
      "    $3",
      "  });",
      "});"
    ],
    "description": "scaffold a jest test"
  },
  "it": {
    "prefix": ["it", "test"],
    "body": ["it(\"$1\", function () {", "  $2", "});"],
    "description": "it tests"
  },
  "filename": {
    "prefix": ["fname"],
    "body": "$TM_FILENAME_BASE"
  }
}
